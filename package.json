{
  "name": "react-form-widgets",
  "version": "0.4.0",
  "description": "Collection of form widgets for React",
  "repository": {
    "url": "https://github.com/viralganatra/react-form-widgets.git",
    "type": "git"
  },
  "author": "Viral Ganatra <viral@viralganatra.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "compile": "babel src -d lib --ignore **/*.test.js,**/__mocks__/*.js",
    "clean": "rimraf lib",
    "test": "jest --onlyChanged",
    "test:ci": "npm run lint && npm run test",
    "lint:src": "eslint src --ignore-pattern '**/*.test.js'",
    "lint:tests": "eslint **/__tests__/*.js -c .eslintrc-test",
    "lint": "npm run lint:src && npm run lint:tests"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "enzyme": "^2.6.0",
    "enzyme-to-json": "^1.3.0",
    "eslint": "^3.11.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-no-unused-vars-rest": "^1.0.4",
    "eslint-plugin-react": "^6.7.1",
    "jest": "^17.0.3",
    "jest-cli": "^17.0.3",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "react": "^0.14 || ^15.0"
  }
}
